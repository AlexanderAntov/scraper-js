{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "allowJs": true,
      "declaration": true,
      "outDir": "lib",
      "strict": true,
      "strictNullChecks": false,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
    },
    "exclude": [
      "./caches",
      "./dist",
      "./dist",
      "./lib",
      "./node_modules"
  ],
    "include": [
      "./src",
      "./src/**/*.ts",
      "./src/**/*.js"
    ]
  }