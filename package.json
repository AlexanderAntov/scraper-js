    "@babel/preset-typescript": "7.15.0",
    "@types/cheerio": "0.22.0",
    "@types/express": "4.17.13",
    "@types/lodash": "4.14.176",
    "@types/node-schedule": "1.3.2",
    "@types/nodemailer": "6.4.4",
    "@types/xml2js": "0.4.9",
    "cheerio": "0.22.0",
    "core-js": "^2.6.11",
    "cpx": "1.5.0",
    "express": "4.18.2",
    "lodash": "4.17.21",
    "node-schedule": "1.3.2",
    "nodemailer": "^6.7.1",
    "regenerator-runtime": "^0.13.9",
